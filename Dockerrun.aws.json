{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "jaychangha/template-client",
      "hostname": "client",
      "essential": false,
      "memory": 2000
    },
    {
      "name": "server",
      "image": "jaychangha/template-server",
      "hostname": "api",
      "essential": false,
      "memory": 2000
    },
    {
      "name": "nginx",
      "image": "jaychangha/template-nginx",
      "essential": true,
      "memory": 2000,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    },
    {
      "name": "worker",
      "image": "jaychangha/template-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 2000
    }
  ]
}